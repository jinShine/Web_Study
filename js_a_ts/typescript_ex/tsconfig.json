{
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "module": "commonjs",
    "rootDir": "src",
    "baseUrl": ".",
    "outDir": "dist",
    "target": "ES5",
    "sourceMap": true,
    "downlevelIteration": true,
    // "noImplicitAny": true, // Any형태의 타입을 방지한다.
    "paths": { "*": ["node_modules/*"] }
  }
}
